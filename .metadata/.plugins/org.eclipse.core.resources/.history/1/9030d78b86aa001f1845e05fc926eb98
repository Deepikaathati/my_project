 public class LoopsREV {
public static void printNumbers(int start, int end) throws InterruptedException {
	  System.out.println("start...");
	  while(start >= end) {
		  System.out.println("count:" +start);
		  Thread.sleep(1000*1);
		  start--;
	  }
	  System.out.println( "end...");
}  
