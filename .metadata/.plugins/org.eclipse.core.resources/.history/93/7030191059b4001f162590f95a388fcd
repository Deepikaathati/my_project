package collections;

import java.util.HashMap;
 //to store key and 

public class HashMapDemo {

	public static void main(String[] args) {
    HashMap<String, Interger > hm = new HashMap<>();		
    hm.put("JFS",400);
    hm.put("PFS",400);
    hm.put("MEAN",400);
    hm.put("MERN",400);
    System.out.println(hm);
    
    hm.put("JFE",500);
    System.out.println(hm);
    

	}

}
