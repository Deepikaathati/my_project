
public class Breakcontinuedemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        for (int i=1; i<=30; i++) {
        	if(i%2==0) {
        		continue;
        	}
        	if(i==15) {
        		break;
        	}
        	System.out.println(i);
        }
	}

}
